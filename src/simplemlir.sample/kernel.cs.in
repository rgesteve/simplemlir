namespace KernelTest
{
    public class KernelClass
    {
        static Tensor Dot(Tensor X, Tensor Y) {
            TensorDim I, J, K;
            TensorIndex i('i'), j('j'), k('k');
            X.bind_dims(I, K);
            Y.bind_dims(K, J);
            var R = TensorOutput(I, J);
            var temp = X.at(i, k) * Y.at(k, j); 
            R.add_at(i, j, temp);
            return R;
        }
    }
}